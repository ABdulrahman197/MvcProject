@model RoleViewModel
@{
    ViewData["Title"] = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Edit Of @Model.RoleName</h1>





<form asp-action="Edit">
        <div class="form-group">
        <label class action="col-form-label" asp-for = "RoleName"></label>
        <input  class ="form-control" asp-for ="RoleName"/> 

    </div>



    <div class="form-group">
        <label class action="col-form-label" asp-for = "Id"></label>
        <input readonly  class ="form-control" asp-for ="Id"/> 

    </div>



  


      <input type="submit" value="Save" class="btn btn-warning" />

        <a asp-action="Index" class = "btn btn-light">Cancel</a>

</form>


