@model RoleViewModel

@{
    ViewData["Title"] = "Delete";
}

<h1>Are You Share to Delete @Model.RoleName User</h1>
<div asp-validation-summary="All"></div>
<form asp-action="Delete"> 

    <div class="form-group">
        <label class action="col-form-label" asp-for = "RoleName"></label>
        <input readonly class ="form-control" asp-for ="RoleName"/> 
    </div>

    <div class="form-group">
        <label class action="col-form-label" asp-for = "Id"></label>
        <input readonly class ="form-control" asp-for ="Id"/> 

    </div>

    <input type="submit" value="Delete" class="btn btn-danger"/>
      <a asp-action="Index" class = "btn btn-light">Cancel</a>

</form>







