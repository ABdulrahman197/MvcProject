@model Employee
@{
    ViewData["Title"] = "Delete";
}

<h1> Are You Sure to Delete @Model.Name</h1>
<br/>
<div asp-validation-summary="All"></div>
<form asp-action="Delete">
    <div class="form-group">
        <label class action="col-form-label" asp-for = "Name "></label>
        <input readonly class ="form-control" asp-for ="Name"/> 

    </div>

    <div class="form-group">
        <label class action="col-form-label" asp-for = "Age"></label>
        <input readonly class ="form-control" asp-for ="Age"/> 

    </div>

    <div class="form-group">
        <label class action="col-form-label" asp-for = "Address"></label>
        <input readonly class ="form-control" asp-for ="Address"/> 

    </div>


    <div class="form-group">
        <label class action="col-form-label" asp-for = "Salary"></label>
        <input readonly class ="form-control" asp-for ="Salary"/> 

    </div>


    <div class="form-group">
        <label class action="col-form-label" asp-for = "IsActive"></label>
        <input readonly class ="form-control" asp-for ="IsActive"/> 

    </div>


     <div class="form-group">
        <label class action="col-form-label" asp-for = "Email"></label>
        <input readonly class ="form-control" asp-for ="Email"/> 

    </div>



     <div class="form-group">
        <label class action="col-form-label" asp-for = "PhoneNumber"></label>
        <input readonly class ="form-control" asp-for ="PhoneNumber"/> 

    </div>





     <div class="form-group">
        <label class action="col-form-label" asp-for = "DateOfCreation"></label>
        <input  readonly class ="form-control" asp-for ="DateOfCreation"/> 

    </div>


       <input type="submit" value="Delete" class="btn btn-danger"/>
        <a asp-action="Index" class = "btn btn-light">Cancel</a>
</form>
   
