@model UserViewModel

@{
    ViewData["Title"] = "Delete";
}

<h1>Are You Share to Delete @Model.Fname User</h1>
<div asp-validation-summary="All"></div>
<form asp-action="Delete"> 

    <div class="form-group">
        <label class action="col-form-label" asp-for = "Fname"></label>
        <input readonly class ="form-control" asp-for ="Fname"/> 
    </div>

    <div class="form-group">
        <label class action="col-form-label" asp-for = "Lname"></label>
        <input readonly class ="form-control" asp-for ="Lname"/> 

    </div>

    <div class="form-group">
        <label class action="col-form-label" asp-for = "Email"></label>
        <input readonly class ="form-control" asp-for ="Email"/> 

    </div>




    <div class="form-group">
        <label class action="col-form-label" asp-for = "PhoneNumber"></label>
        <input readonly class ="form-control" asp-for ="PhoneNumber"/> 

    </div>



    <div class="form-group">
        <label class action="col-form-label" asp-for = "Roles"></label>
        <input readonly class ="form-control" asp-for ="Roles"/> 

    </div>
    <input type="submit" value="Delete" class="btn btn-danger"/>
      <a asp-action="Index" class = "btn btn-light">Cancel</a>

</form>






